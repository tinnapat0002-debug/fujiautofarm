local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Auto Farm",
    SubTitle = "by ZeroVertor101",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "box" }),
    Misc = Window:AddTab({ Title = "Misc", Icon = "heart" })
}

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local VirtualUser = game:GetService("VirtualUser")
local player = Players.LocalPlayer

local Char = player.Character or player.CharacterAdded:Wait()
local hrp = Char:WaitForChild("HumanoidRootPart")

local BoxFolder = workspace.Farm.Box

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°
local autoFarmEnabled = false
local checkingStats = false
local autoSellEnabled = true
local antiAfkEnabled = true
local lastSellTime = 0  -- ‡πÄ‡∏ß‡∏•‡∏≤‡∏Ç‡∏≤‡∏¢‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î
local sellCooldown = 10  -- delay 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ

--========================================================--
--                    ANTI AFK SYSTEM                     
--========================================================--

-- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô Kick ‡∏à‡∏≤‡∏Å AFK
local function setupAntiAFK()
    -- ‡∏•‡∏ö Connection ‡πÄ‡∏î‡∏¥‡∏°‡∏Å‡πà‡∏≠‡∏ô (‡∏ñ‡πâ‡∏≤‡∏°‡∏µ)
    if getconnections then
        for _, connection in pairs(getconnections(player.Idled)) do
            connection:Disable()
        end
    end
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Connection ‡πÉ‡∏´‡∏°‡πà
    player.Idled:Connect(function()
        if antiAfkEnabled then
            VirtualUser:CaptureController()
            VirtualUser:ClickButton2(Vector2.new())
            print("üõ°Ô∏è Anti AFK: Prevented kick!")
        end
    end)
    
    print("‚úÖ Anti AFK System Enabled")
end

-- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÉ‡∏ä‡πâ Anti AFK
setupAntiAFK()

-- Anti AFK ‡πÄ‡∏™‡∏£‡∏¥‡∏° (‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏∏‡∏Å 5 ‡∏ô‡∏≤‡∏ó‡∏µ)
task.spawn(function()
    while true do
        task.wait(300) -- 5 ‡∏ô‡∏≤‡∏ó‡∏µ
        
        if antiAfkEnabled then
            pcall(function()
                VirtualUser:CaptureController()
                VirtualUser:ClickButton2(Vector2.new())
                VirtualUser:Button1Down(Vector2.new(0, 0))
                task.wait(0.1)
                VirtualUser:Button1Up(Vector2.new(0, 0))
                print("üõ°Ô∏è Anti AFK: Activity simulated")
            end)
        end
    end
end)

--========================================================--
--                  MAIN FUNCTIONS
--========================================================--

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡πÄ‡∏Å‡∏≤‡∏∞‡∏Å‡∏•‡πà‡∏≠‡∏á
local function ToBoxIsland()
    if not hrp then return end
    hrp.CFrame = CFrame.new(-1707.47729, -19.5721149, 1004.60181)
    print("‚úÖ Warped to Box Island")
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏£‡πâ‡∏≤‡∏ô
local function toShop()
    if not hrp then return end
    hrp.CFrame = CFrame.new(595.987793, 13.514061, -119.548019)
    print("üè™ Warped to Shop")
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ã‡∏∑‡πâ‡∏≠‡∏≠‡∏≤‡∏´‡∏≤‡∏£
local function buyFood()
    print("üí∞ Buying food...")
    
    ReplicatedStorage.Remote.Shop:FireServer("BuyCash", "Coke", 1)
    task.wait(1)
    
    ReplicatedStorage.Remote.Shop:FireServer("BuyCash", "Lunchbox", 1)
    
    print("‚úÖ Food purchased!")
    task.wait(3)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Å‡∏¥‡∏ô‡∏≠‡∏≤‡∏´‡∏≤‡∏£
local function eatFood()
    print("üç¥ Eating food...")
    
    ReplicatedStorage.Remote.Inventory:FireServer("Use", "Coke")
    task.wait(1)
    
    local ohString1 = "Use"
    local ohString2 = "Lunchbox"

    game:GetService("ReplicatedStorage").Remote.Inventory:FireServer(ohString1, ohString2)
    task.wait(1)
    
    print("‚úÖ Food consumed!")
end


local function sellBoxes()
    pcall(function()
        local currentTime = tick()
        if currentTime - lastSellTime < sellCooldown then
            return
        end

        local inventory = player:FindFirstChild("Inventory")
        if not inventory then return end

        local boxValue = inventory:FindFirstChild("Box")
        if not boxValue then return end

        local boxCount = boxValue.Value

        print("üì¶ Current boxes: " .. boxCount)

        if boxCount >= 60 then
            print("üí∞ Selling boxes...")

            -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏à‡∏∏‡∏î‡∏Ç‡∏≤‡∏¢
            hrp.CFrame = CFrame.new(4441, 5, -2181)
            task.wait(10)

            -- ‡∏Ç‡∏≤‡∏¢‡∏Å‡∏•‡πà‡∏≠‡∏á
            ReplicatedStorage.Remote.Economy:FireServer("Sell", "Box", "60")

            -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏ß‡∏•‡∏≤‡∏Ç‡∏≤‡∏¢‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î
            lastSellTime = tick()

            print("‚úÖ Sold 60 boxes!")

            Fluent:Notify({
                Title = "Auto Sell",
                Content = "‡∏Ç‡∏≤‡∏¢‡∏Å‡∏•‡πà‡∏≠‡∏á 60 ‡∏ä‡∏¥‡πâ‡∏ô‡πÅ‡∏•‡πâ‡∏ß!",
                Duration = 3
            })

            -- ‡∏£‡∏≠ 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ ‡πÅ‡∏•‡πâ‡∏ß‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏ü‡∏≤‡∏£‡πå‡∏°
            task.wait(10)
            ToBoxIsland()
        end
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ä‡πá‡∏Ñ‡πÅ‡∏•‡∏∞‡∏ã‡∏∑‡πâ‡∏≠/‡∏Å‡∏¥‡∏ô‡∏≠‡∏≤‡∏´‡∏≤‡∏£
local function checkAndEat()
    pcall(function()
        local status = player:FindFirstChild("Status")
        if not status then return end
        
        local hunger = status:FindFirstChild("Hunger")
        local thirsty = status:FindFirstChild("Thirsty")
        
        if not hunger or not thirsty then return end
        
        local hungerValue = hunger.Value
        local thirstyValue = thirsty.Value
        
        print("üìä Hunger: " .. hungerValue .. " | Thirsty: " .. thirstyValue)
        
        -- ‡∏ñ‡πâ‡∏≤‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 20
        if hungerValue < 20 or thirstyValue < 20 then
            print("‚ö†Ô∏è Low stats detected! Going to shop...")
            
            toShop()
            task.wait(2)
            
            buyFood()
            task.wait(2)
            
            eatFood()
            task.wait(2)
            
            print("üîÑ Returning to farm...")
            ToBoxIsland()
            task.wait(1)
        end
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏Å‡∏•‡πà‡∏≠‡∏á
local function tpBox()
    for _, v in pairs(BoxFolder:GetChildren()) do
        if not autoFarmEnabled then break end
        
        if v.Name == "Box" and v:IsA("BasePart") then
            print("üì¶ Farming box: " .. v.Name)
            hrp.CFrame = v.CFrame + Vector3.new(0, 3, 0)
            task.wait(5)
            
            -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏´‡∏•‡∏±‡∏á‡πÄ‡∏Å‡πá‡∏ö‡∏Å‡∏•‡πà‡∏≠‡∏á
            checkAndEat()
            
            -- ‡πÄ‡∏ä‡πá‡∏Ñ‡πÅ‡∏•‡∏∞‡∏Ç‡∏≤‡∏¢‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏ñ‡πâ‡∏≤‡∏Ñ‡∏£‡∏ö 60
            if autoSellEnabled then
                sellBoxes()
            end
        end
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Auto Farm ‡∏´‡∏•‡∏±‡∏Å
local function startAutoFarm()
    while autoFarmEnabled do
        task.wait(1)
        
        pcall(function()
            Char = player.Character or player.CharacterAdded:Wait()
            hrp = Char:WaitForChild("HumanoidRootPart")
            
            -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏Ç‡∏≤‡∏¢‡∏Å‡πà‡∏≠‡∏ô‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÉ‡∏´‡∏°‡πà
            if autoSellEnabled then
                sellBoxes()
            end
            
            ToBoxIsland()
            task.wait(2)
            
            tpBox()
            task.wait(2)
        end)
    end
end

-- ‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏ä‡πá‡∏Ñ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
local function autoCheckStats()
    while checkingStats do
        task.wait(10)

        if autoFarmEnabled then
            checkAndEat()

            -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏Ç‡∏≤‡∏¢‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏î‡πâ‡∏ß‡∏¢
            if autoSellEnabled then
                sellBoxes()
            end
        end
    end
end

--========================================================--
--                  UI ELEMENTS - MAIN TAB                
--========================================================--

local Toggle = Tabs.Main:AddToggle("AutoFarm", {
    Title = "Auto Farm Box",
    Description = "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥",
    Default = false
})

Toggle:OnChanged(function(value)
    autoFarmEnabled = value
    
    if value then
        print("üöÄ Auto Farm Started!")
        Fluent:Notify({
            Title = "Auto Farm",
            Content = "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏Å‡∏•‡πà‡∏≠‡∏á!",
            Duration = 3
        })
        
        task.spawn(startAutoFarm)
    else
        print("‚èπÔ∏è Auto Farm Stopped!")
        Fluent:Notify({
            Title = "Auto Farm",
            Content = "‡∏´‡∏¢‡∏∏‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏Å‡∏•‡πà‡∏≠‡∏á!",
            Duration = 3
        })
    end
end)

local AutoSellToggle = Tabs.Main:AddToggle("AutoSell", {
    Title = "Auto Sell Box (60)",
    Description = "‡∏Ç‡∏≤‡∏¢‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ 60 ‡∏ä‡∏¥‡πâ‡∏ô",
    Default = true
})

