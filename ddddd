local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Auto Farm",
    SubTitle = "by ZeroVertor101",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "box" }),
    Misc = Window:AddTab({ Title = "Misc", Icon = "heart" })
}

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local VirtualUser = game:GetService("VirtualUser")
local player = Players.LocalPlayer

local Char = player.Character or player.CharacterAdded:Wait()
local hrp = Char:WaitForChild("HumanoidRootPart")

local BoxFolder = workspace.Farm.Box

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°
local autoFarmEnabled = false
local checkingStats = false
local autoSellEnabled = true
local antiAfkEnabled = true
local lastSellTime = 0  -- ‡πÄ‡∏ß‡∏•‡∏≤‡∏Ç‡∏≤‡∏¢‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î
local sellCooldown = 10  -- delay 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
local isBusy = false  -- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏ã‡πâ‡∏≠‡∏ô‡∏ó‡∏±‡∏ö

--========================================================--
--                    ANTI AFK SYSTEM                     
--========================================================--

-- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô Kick ‡∏à‡∏≤‡∏Å AFK
local function setupAntiAFK()
    -- ‡∏•‡∏ö Connection ‡πÄ‡∏î‡∏¥‡∏°‡∏Å‡πà‡∏≠‡∏ô (‡∏ñ‡πâ‡∏≤‡∏°‡∏µ)
    if getconnections then
        for _, connection in pairs(getconnections(player.Idled)) do
            connection:Disable()
        end
    end
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á Connection ‡πÉ‡∏´‡∏°‡πà
    player.Idled:Connect(function()
        if antiAfkEnabled then
            VirtualUser:CaptureController()
            VirtualUser:ClickButton2(Vector2.new())
            print("üõ°Ô∏è Anti AFK: Prevented kick!")
        end
    end)
    
    print("‚úÖ Anti AFK System Enabled")
end

-- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÉ‡∏ä‡πâ Anti AFK
setupAntiAFK()

-- Anti AFK ‡πÄ‡∏™‡∏£‡∏¥‡∏° (‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏∏‡∏Å 5 ‡∏ô‡∏≤‡∏ó‡∏µ)
task.spawn(function()
    while true do
        task.wait(300) -- 5 ‡∏ô‡∏≤‡∏ó‡∏µ
        
        if antiAfkEnabled then
            pcall(function()
                VirtualUser:CaptureController()
                VirtualUser:ClickButton2(Vector2.new())
                VirtualUser:Button1Down(Vector2.new(0, 0))
                task.wait(0.1)
                VirtualUser:Button1Up(Vector2.new(0, 0))
                print("üõ°Ô∏è Anti AFK: Activity simulated")
            end)
        end
    end
end)

--========================================================--
--            BYPASS MOVEMENT (WALKING SIM)              
--========================================================--
local walkSpeedValue = 60 
local RunService = game:GetService("RunService")

local function walkToBypass(targetPos)
    local char = player.Character
    if not char or isBusy then return end 
    local hrp = char:FindFirstChild("HumanoidRootPart")
    local hum = char:FindFirstChildOfClass("Humanoid")
    if not hrp or not hum then return end

    local finalTarget = targetPos
    local distance = (hrp.Position - finalTarget).Magnitude
    
    -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏á‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏Ç and not isBusy ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏´‡∏¢‡∏∏‡∏î‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡∏ñ‡πâ‡∏≤‡∏ï‡πâ‡∏≠‡∏á‡πÑ‡∏õ‡∏Ç‡∏≤‡∏¢‡∏Ç‡∏≠‡∏á
    while distance > 0.5 and autoFarmEnabled and not isBusy do
        RunService.Heartbeat:Wait()
        
        if isBusy or not hrp then break end -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ã‡πâ‡∏≥‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
        
        local currentPos = hrp.Position
        local direction = (finalTarget - currentPos).Unit
        distance = (currentPos - finalTarget).Magnitude
        
        local nextPos = currentPos + (direction * (walkSpeedValue / 60))
        
        -- ‡∏•‡πá‡∏≠‡∏Å‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏ï‡∏≤‡∏°‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏Å‡∏•‡πà‡∏≠‡∏á + ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡∏ï‡∏Å‡πÅ‡∏°‡∏û
        hrp.CFrame = CFrame.new(Vector3.new(nextPos.X, targetPos.Y, nextPos.Z))
        hrp.CFrame = CFrame.lookAt(hrp.Position, Vector3.new(finalTarget.X, hrp.Position.Y, finalTarget.Z))
        
        hum:MoveTo(hrp.Position + direction)
        
        pcall(function()
            for _, part in pairs(char:GetDescendants()) do
                if part:IsA("BasePart") then part.CanCollide = false end
            end
        end)
        hrp.Velocity = Vector3.new(0, 0, 0)
    end
    
    if not isBusy then
        hrp.CFrame = CFrame.new(targetPos) -- Snap ‡πÄ‡∏Ç‡πâ‡∏≤‡∏´‡∏≤‡∏û‡∏¥‡∏Å‡∏±‡∏î‡πÄ‡∏õ‡πä‡∏∞‡πÜ
    end
end

--========================================================--
--                  MAIN FUNCTIONS
--========================================================--

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡πÄ‡∏Å‡∏≤‡∏∞‡∏Å‡∏•‡πà‡∏≠‡∏á
local function ToBoxIsland()
    local char = player.Character
    local root = char and char:FindFirstChild("HumanoidRootPart")
    if root then
        root.CFrame = CFrame.new(-1707.47729, -19.5721149, 1004.60181)
        print("‚úÖ Warped to Box Island")
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏£‡πâ‡∏≤‡∏ô
local function toShop()
    if not hrp then return end
    hrp.CFrame = CFrame.new(595.987793, 13.514061, -119.548019)
    print("üè™ Warped to Shop")
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ã‡∏∑‡πâ‡∏≠‡∏≠‡∏≤‡∏´‡∏≤‡∏£
local function buyFood()
    print("üí∞ Buying food...")
    
    ReplicatedStorage.Remote.Shop:FireServer("BuyCash", "Coke", 1)
    task.wait(1)
    
    ReplicatedStorage.Remote.Shop:FireServer("BuyCash", "Lunchbox", 1)
    
    print("‚úÖ Food purchased!")
    task.wait(3)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Å‡∏¥‡∏ô‡∏≠‡∏≤‡∏´‡∏≤‡∏£
local function eatFood()
    print("üç¥ Eating food...")
    
    ReplicatedStorage.Remote.Inventory:FireServer("Use", "Coke")
    task.wait(15)
    
    local ohString1 = "Use"
    local ohString2 = "Lunchbox"

    game:GetService("ReplicatedStorage").Remote.Inventory:FireServer(ohString1, ohString2)
    task.wait(15)
    
    print("‚úÖ Food consumed!")
end


local function sellBoxes()
    if isBusy then return end

    local inventory = player:FindFirstChild("Inventory")
    local boxValue = inventory and inventory:FindFirstChild("Box")
    
    if boxValue and boxValue.Value >= 60 then
        isBusy = true
        print("üí∞ [System] ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Ç‡∏±‡πâ‡∏ô‡∏ï‡∏≠‡∏ô‡∏Å‡∏≤‡∏£‡∏Ç‡∏≤‡∏¢...")
        
        task.wait(0.5)
        local root = player.Character:FindFirstChild("HumanoidRootPart")
        if root then
            root.CFrame = CFrame.new(4441, 5, -2181)
            task.wait(7)
            ReplicatedStorage.Remote.Economy:FireServer("Sell", "Box", "60")
            print("‚úÖ Sold 60 boxes!")
            lastSellTime = tick()
            task.wait(3)
            
            ToBoxIsland()
            task.wait(2)
        end
        isBusy = false
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏ä‡πá‡∏Ñ‡πÅ‡∏•‡∏∞‡∏ã‡∏∑‡πâ‡∏≠/‡∏Å‡∏¥‡∏ô‡∏≠‡∏≤‡∏´‡∏≤‡∏£
local function checkAndEat()
    if isBusy then return end

    local status = player:FindFirstChild("Status")
    local hunger = status and status:FindFirstChild("Hunger")
    local thirsty = status and status:FindFirstChild("Thirsty")

    if hunger and thirsty and (hunger.Value < 50 or thirsty.Value < 50) then
        isBusy = true -- ‡∏•‡πá‡∏≠‡∏Å‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
        print("‚ö†Ô∏è [System] ‡∏´‡∏¥‡∏ß/‡∏Å‡∏£‡∏∞‡∏´‡∏≤‡∏¢ ‡πÑ‡∏õ‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤...")

        toShop()
        task.wait(7)

        -- ‡∏ß‡∏ô‡∏ã‡∏∑‡πâ‡∏≠/‡∏Å‡∏¥‡∏ô‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤ Hunger ‡πÅ‡∏•‡∏∞ Thirsty ‡∏à‡∏∞‡∏°‡∏≤‡∏Å‡∏Å‡∏ß‡πà‡∏≤ 50
        while true do
            -- ‡∏≠‡πà‡∏≤‡∏ô‡∏Ñ‡πà‡∏≤‡πÉ‡∏´‡∏°‡πà
            status = player:FindFirstChild("Status")
            hunger = status and status:FindFirstChild("Hunger")
            thirsty = status and status:FindFirstChild("Thirsty")

            if not hunger or not thirsty then break end

            local hungerVal = hunger.Value
            local thirstyVal = thirsty.Value

            print("üìä Hunger: " .. hungerVal .. " | Thirsty: " .. thirstyVal)

            -- ‡∏ñ‡πâ‡∏≤‡∏ó‡∏±‡πâ‡∏á‡∏™‡∏≠‡∏á‡∏Ñ‡πà‡∏≤‡∏°‡∏≤‡∏Å‡∏Å‡∏ß‡πà‡∏≤ 50 ‡πÅ‡∏•‡πâ‡∏ß ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏¥‡∏ô
            if hungerVal > 50 and thirstyVal > 50 then
                print("‚úÖ ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÄ‡∏ï‡πá‡∏°‡πÅ‡∏•‡πâ‡∏ß! ‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏ü‡∏≤‡∏£‡πå‡∏°...")
                break
            end

            -- ‡∏ã‡∏∑‡πâ‡∏≠‡∏≠‡∏≤‡∏´‡∏≤‡∏£
            buyFood()
            task.wait(2)

            -- ‡∏Å‡∏¥‡∏ô‡∏≠‡∏≤‡∏´‡∏≤‡∏£
            eatFood()
            task.wait(2)
        end

        ToBoxIsland() -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡∏•‡∏±‡∏ö‡πÄ‡∏Å‡∏≤‡∏∞‡∏Å‡πà‡∏≠‡∏ô‡∏õ‡∏•‡∏î‡∏•‡πá‡∏≠‡∏Å
        task.wait(2)
        isBusy = false -- ‡∏õ‡∏•‡∏î‡∏•‡πá‡∏≠‡∏Å
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏î‡∏¥‡∏ô‡πÄ‡∏Å‡πá‡∏ö‡∏Å‡∏•‡πà‡∏≠‡∏á (‡πÄ‡∏ä‡πá‡∏Ñ Transparency ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÑ‡∏ß‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î)
local function tpBox()
    -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏¢‡πà‡∏≠‡∏¢‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏´‡∏≤‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î
    local function getClosestBox()
        local closest = nil
        local maxDist = math.huge
        local char = player.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        
        if not hrp then return nil end
        
        for _, v in pairs(BoxFolder:GetChildren()) do
            if v.Name == "Box" and v:IsA("BasePart") then
                -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏Å‡πà‡∏≠‡∏ô‡∏ß‡πà‡∏≤‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏ô‡∏±‡πâ‡∏ô‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏´‡∏≤‡∏¢‡πÑ‡∏õ (Transparency ‡∏ï‡πâ‡∏≠‡∏á‡∏ô‡πâ‡∏≠‡∏¢‡∏Å‡∏ß‡πà‡∏≤ 1)
                if v.Transparency < 1 then
                    local dist = (v.Position - hrp.Position).Magnitude
                    if dist < maxDist then
                        maxDist = dist
                        closest = v
                    end
                end
            end
        end
        return closest
    end

    -- ‡∏•‡∏π‡∏õ‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ï‡πà‡∏≠‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á
    while autoFarmEnabled and not isBusy do
        local targetBox = getClosestBox()
        
        if targetBox then
            print("üö∂ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏î‡∏¥‡∏ô‡πÑ‡∏õ‡∏´‡∏≤‡∏Å‡∏•‡πà‡∏≠‡∏á: " .. targetBox.Name)
            

            walkToBypass(targetBox.Position)
            

            print("‚è≥ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÄ‡∏Å‡πá‡∏ö‡∏Å‡∏•‡πà‡∏≠‡∏á... ‡∏£‡∏≠‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤‡∏à‡∏∞‡∏´‡∏≤‡∏¢‡πÑ‡∏õ")
            
            local timeout = 0

            while targetBox and targetBox.Parent and targetBox.Transparency < 1 and autoFarmEnabled do
                task.wait(0.1)
                timeout = timeout + 0.1
                

                if timeout > 15 then break end 
            end
            
            print("‚úÖ ‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏´‡∏≤‡∏¢‡πÑ‡∏õ‡πÅ‡∏•‡πâ‡∏ß! ‡πÑ‡∏õ‡∏ï‡πà‡∏≠...")
            
            -- 3. ‡πÄ‡∏ä‡πá‡∏Ñ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
            checkAndEat()
            if autoSellEnabled then 
                sellBoxes() 
            end
        else
            -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏ó‡∏µ‡πà‡πÄ‡∏Å‡πá‡∏ö‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏¢ ‡πÉ‡∏´‡πâ‡∏£‡∏≠ 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡πÅ‡∏•‡πâ‡∏ß‡∏´‡∏≤‡πÉ‡∏´‡∏°‡πà
            task.wait(1)
        end
    end
end
-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Auto Farm ‡∏´‡∏•‡∏±‡∏Å (‡πÄ‡∏ä‡πá‡∏Å‡∏£‡∏∞‡∏¢‡∏∞‡∏´‡πà‡∏≤‡∏á‡∏Å‡πà‡∏≠‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡πÄ‡∏Å‡∏≤‡∏∞)
local function startAutoFarm()
    while autoFarmEnabled do
        task.wait(0.5)
        pcall(function()
            local char = player.Character
            local hrp = char:FindFirstChild("HumanoidRootPart")
            if not isBusy then
                local islandPos = Vector3.new(-1707.47, -19.57, 1004.60)
                    ToBoxIsland()
                    task.wait(1)
                tpBox()
            end
        end)
    end
end

-- ‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏ä‡πá‡∏Ñ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
local function autoCheckStats()
    while checkingStats do
        task.wait(10)

        if autoFarmEnabled then
            checkAndEat()

            -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏Ç‡∏≤‡∏¢‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏î‡πâ‡∏ß‡∏¢
            if autoSellEnabled then
                sellBoxes()
            end
        end
    end
end

