            Duration = 2
        })
    else
        Fluent:Notify({
            Title = "Auto Sell",
            Content = "‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏Ç‡∏≤‡∏¢‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥!",
            Duration = 2
        })
    end
end)

local AutoCheckToggle = Tabs.Main:AddToggle("AutoCheck", {
    Title = "Auto Check Stats",
    Description = "‡πÄ‡∏ä‡πá‡∏Ñ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡∏ó‡∏∏‡∏Å 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ",
    Default = true
})

AutoCheckToggle:OnChanged(function(value)
    checkingStats = value
    
    if value then
        task.spawn(autoCheckStats)
    end
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏î‡πà‡∏ß‡∏ô
Tabs.Main:AddButton({
    Title = "Warp to Box Island",
    Description = "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡πÄ‡∏Å‡∏≤‡∏∞‡∏Å‡∏•‡πà‡∏≠‡∏á",
    Callback = function()
        ToBoxIsland()
