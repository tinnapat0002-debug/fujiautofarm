            Duration = 2
        })
    end
})

Tabs.Main:AddButton({
    Title = "Warp to Shop",
    Description = "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏£‡πâ‡∏≤‡∏ô",
    Callback = function()
        toShop()
        Fluent:Notify({
            Title = "Teleport",
            Content = "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏£‡πâ‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß!",
            Duration = 2
        })
    end
})

Tabs.Main:AddButton({
    Title = "Buy & Eat Food",
    Description = "‡∏ã‡∏∑‡πâ‡∏≠‡πÅ‡∏•‡∏∞‡∏Å‡∏¥‡∏ô‡∏≠‡∏≤‡∏´‡∏≤‡∏£‡∏ó‡∏±‡∏ô‡∏ó‡∏µ",
    Callback = function()
        local currentPos = hrp.CFrame
        toShop()
        task.wait(1)
        buyFood()
        task.wait(1)
        eatFood()
        task.wait(1)
        hrp.CFrame = currentPos
        
        Fluent:Notify({
            Title = "Food",
            Content = "‡∏ã‡∏∑‡πâ‡∏≠‡πÅ‡∏•‡∏∞‡∏Å‡∏¥‡∏ô‡∏≠‡∏≤‡∏´‡∏≤‡∏£‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢!",
            Duration = 2
        })
    end
})

Tabs.Main:AddButton({
    Title = "Sell Boxes Now",
    Description = "‡∏Ç‡∏≤‡∏¢‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ (60 ‡∏ä‡∏¥‡πâ‡∏ô)",
    Callback = function()
        sellBoxes()
    end
})

-- ‡πÅ‡∏™‡∏î‡∏á‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
local StatusSection = Tabs.Main:AddSection("Current Status")

local BoxCountParagraph = Tabs.Main:AddParagraph({
    Title = "Box Count",
    Content = "Loading..."
})

task.spawn(function()
    while true do
        task.wait(2)
        
        pcall(function()
            local status = player:FindFirstChild("Status")
            local inventory = player:FindFirstChild("Inventory")
            
            if status and inventory then
                local hunger = status:FindFirstChild("Hunger")
                local thirsty = status:FindFirstChild("Thirsty")
                local boxValue = inventory:FindFirstChild("Box")
                
                -- ‡πÄ‡∏ä‡πá‡∏Ñ‡πÄ‡∏•‡∏∑‡∏≠‡∏î
                local humanoid = Char:FindFirstChild("Humanoid")
                local health = humanoid and math.floor(humanoid.Health) or 0
                local maxHealth = humanoid and math.floor(humanoid.MaxHealth) or 100
                
                if hunger and thirsty and boxValue then
                    local boxCount = boxValue.Value
                    
                    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
                    BoxCountParagraph:SetTitle("Current Status")
                    BoxCountParagraph:SetDesc(string.format(
                        "üì¶ Boxes: %d/60\nüíö HP: %d/%d\nüçñ Hunger: %d\nüíß Thirsty: %d",
                        boxCount,
                        health,
                        maxHealth,
                        math.floor(hunger.Value),
                        math.floor(thirsty.Value)
                    ))
                end
            end
        end)
    end
end)

