    end
end)

local AutoCheckToggle = Tabs.Main:AddToggle("AutoCheck", {
    Title = "Auto Check Stats",
    Description = "‡πÄ‡∏ä‡πá‡∏Ñ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥‡∏ó‡∏∏‡∏Å 10 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ",
    Default = true
})

AutoCheckToggle:OnChanged(function(value)
    checkingStats = value
    
    if value then
        task.spawn(autoCheckStats)
    end
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏î‡πà‡∏ß‡∏ô
