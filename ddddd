local Toggle = Tabs.Main:AddToggle("AutoFarm", {
    Title = "Auto Farm Box",
    Description = "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥",
    Default = false
})

Toggle:OnChanged(function(value)
    autoFarmEnabled = value
    
    if value then
        print("üöÄ Auto Farm Started!")
        Fluent:Notify({
            Title = "Auto Farm",
            Content = "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏Å‡∏•‡πà‡∏≠‡∏á!",
            Duration = 3
        })
        
        task.spawn(startAutoFarm)
    else
        print("‚èπÔ∏è Auto Farm Stopped!")
        Fluent:Notify({
            Title = "Auto Farm",
            Content = "‡∏´‡∏¢‡∏∏‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏Å‡∏•‡πà‡∏≠‡∏á!",
            Duration = 3
        })
    end
end)

